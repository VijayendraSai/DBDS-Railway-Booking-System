<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>Login</display-name>
  <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterEmployeeServlet</servlet-name>
        <servlet-class>com.servlet.RegisterEmployeeServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>trainScheduleSearchServlet</servlet-name>
        <servlet-class>com.servlet.TrainScheduleSearchServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>trainScheduleSearchServlet</servlet-name>
        <url-pattern>/trainScheduleSearchServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>RegisterEmployeeServlet</servlet-name>
        <url-pattern>/registerEmployee</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
</web-app>